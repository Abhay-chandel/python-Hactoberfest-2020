   "source": [
    "# import the needed libs\n",
    "from math import sqrt\n",
    "from itertools import count, islice\n",
    "#check if number is prime\n",
    "def is_prime(n):\n",
    "    return n > 1 and all(n % i for i in islice(count(2), int(sqrt(n)-1)))"
   ]
  },
